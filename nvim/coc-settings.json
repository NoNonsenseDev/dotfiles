{
  "toml.schema.associations": {
    ".*/starship.toml": "https://starship.rs/config-schema.json"
  },
  "list.source.buffers.defaultOptions": [
    "--ignore-case"
  ],
  "coc.source.emoji.filetypes": [
    "markdown",
    "gitcommit"
  ],
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true
  },
  "coc.source.omni.filetypes": [
    "sh",
    "bash",
    "html",
    "css",
    "dockerfile"
  ],
  "pyright.organizeimports.provider": "isort",
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "pydocstring.formatter": "google",
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "json",
    "go",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "dockerfile"
  ],
  "coc.preferences.rootPatterns": [
    ".vim",
    ".git",
    ".projections.json",
    "pubspec.lock",
    "Gopkg.lock",
    "package.json",
    "Gemfile",
    "Rakefile",
    "Makefile",
    "requirements.txt",
    "venv",
    "Pipfile",
    "Pipfile.lock"
  ],
  "git.enableGutters": true,
  "codeLens.enable": true,
  "solargraph.formatting": true,
  "solargraph.useBundler": true
}
