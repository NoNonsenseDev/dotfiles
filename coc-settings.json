{
  "coc.preferences.noselect": false,
  "coc.preferences.minTriggerInputLength": 2,
  "coc.preferences.hoverTarget": "echo",
  "coc.source.tag.enable": true,
  "coc.preferences.triggerSignatureHelp": true,
  "coc.preferences.autoTrigger": "always",
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.preferences.diagnostic.displayByAle": true,
  "coc.preferences.rootPatterns": [
    ".vim",
    ".git",
    ".hg",
    ".projections.json",
    "pubspec.lock",
    "Gopkg.lock",
    "package.json",
    "Gemfile",
    "Rakefile",
    "Makefile",
    "requirements.txt"
  ],
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "filetypes": ["sh"],
      "args": ["start"]
    },
    "golang": {
      "command": "bingo",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "formatStyle": "goimports"
      }
    },
    "dart": {
      "command": "dart_language_server",
      "args": [],
      "rootPatterns": ["pubspec.lock"],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  }
}
