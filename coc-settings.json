{
  "coc.preferences.minTriggerInputLength": 1,
  "coc.preferences.hoverTarget": "echo",
  "coc.source.tag.enable": true,
  "coc.preferences.triggerSignatureHelp": true,
  "coc.preferences.autoTrigger": "always",
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.preferences.diagnostic.displayByAle": true,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "filetypes": ["sh"],
      "args": ["start"]
    },
    "golang": {
      "command": "bingo",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "formatStyle": "goimports"
      }
    },
    "dart": {
      "command": "dart_language_server",
      "args": [],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    }
  }
}
